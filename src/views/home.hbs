<div class="container">
    <h3>Dashboard</h3>  
    <br>
    <div class="container">
        <div class="table-responsive">
            <table class="table table-primary">
                <thead>
                    <tr>
                        <th scope="col">Objeto</th>
                        <th scope="col">Valor Financiado</th>
                        <th scope="col">Valor Pago</th>
                        <th scope="col">Status</th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="">                        
                        {{#with dadosFinanciamento}}
                        <td>{{objeto}}</td>
                        <td>R${{total}}</td>
                        <td>R${{saldo}}</td>
                        <td>Pendente</td>
                        {{/with}}
                    </tr>
                </tbody>
            </table>
        </div>
        
    </div>     
</div>